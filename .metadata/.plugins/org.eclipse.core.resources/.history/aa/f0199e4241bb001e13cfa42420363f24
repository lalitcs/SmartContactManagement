package com.spring.core.practical.constructor.injection;

public class Student {

	private int id;
	private String name;
	private String city;
	private String company;

	public Student(int id, String name, String city, String company) {
		System.out.println("inside construcot");
		this.id=id;
		this.name = name;
		this.city = city;
		this.company = company;
	}

	public Student() {
	}

	@Override
	public String toString() {
		return "Student [id=id, name=" + name + ", city=" + city + ", company=" + company + "]";
	}

//	void disp() {
//		System.out.println(name + ":" + city + ":" + company);
//	}
	

}
