package com.Core.Java.Collectoin.Lalit.list;

import java.util.Vector;

public class VectorDemoByLalit {
	public static void main(String[] args) {
		Vector v=new Vector();
		v.add(10);
		v.add(20);
		v.add(30);
		v.add(40);
		v.add(50);
		v.add(60);
		v.add(70);
		v.add(80);
		v.add(90);
		v.add(100);
		v.add(110);
		System.out.println(v);
		System.out.println("size of vecto: "+v.size());
	
		System.out.println("capacity of  vector: "+v.capacity());
		System.out.println("Traversing vector ");
		for(int i=0;i<v.size();i++) {
		System.out.println(v.get(i));
		}
	}
}
