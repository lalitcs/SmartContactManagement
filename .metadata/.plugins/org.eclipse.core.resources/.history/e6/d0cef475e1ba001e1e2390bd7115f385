 * Hello world!
 *
 */
public class App 
{
    public static void main( String[] args )
    {
        ApplicationContext context=new ClassPathXmlApplicationContext("com/spring/jdbc/practical/config.xml");
        Student st=(Student)context.getBean("Studentdao");
        System.out.println();
    }
}
